{
    "name": "boilerplate-angular-19",
    "version": "0.0.0",
    "scripts": {
        "start:local": "ng serve --port=4600 --configuration local --host 0.0.0.0",
        "start:dev": "ng serve --port=4600 --configuration dev --host 0.0.0.0",
        "start:qa": "ng serve --port=4600 --configuration qa --host 0.0.0.0",
        "start:prod": "ng serve --port=4600 --configuration prod --host 0.0.0.0",
        "build:dev": "ng build --configuration dev",
        "build:qa": "ng build --configuration qa",
        "build:prod": "ng build --configuration prod",
        "test": "ng test",
        "lint": "eslint src/**",
        "format": "prettier --write \"./src/**/*.{ts,json,html}\"",
        "lint-format": "npm run format &&  npm run lint --fix",
        "pre-commit": "npm run lint-format && git add .",
        "prepare": "husky install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^19.2.14",
        "@angular/common": "^19.2.0",
        "@angular/compiler": "^19.2.0",
        "@angular/core": "^19.2.0",
        "@angular/forms": "^19.2.0",
        "@angular/platform-browser": "^19.2.0",
        "@angular/platform-browser-dynamic": "^19.2.0",
        "@angular/router": "^19.2.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.2.14",
        "@angular/cli": "^19.2.14",
        "@angular/compiler-cli": "^19.2.0",
        "@types/jasmine": "~5.1.0",
        "@typescript-eslint/eslint-plugin": "^8.34.0",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.1",
        "husky": "^9.1.7",
        "jasmine-core": "~5.6.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "typescript": "~5.7.2"
    }
}
